{
    "name": "blackpearl",
    "version": "0.0.1",
    "private": true,
    "workspaces": {
      "packages": [
        "packages/*"
      ]
    },
    "license": "MIT",
    "scripts": {
      "build:components": "lerna exec --parallel -- rollup -c ../../rollup.config.js",
      "clean:components": "lerna exec -- rm -rf dist",
